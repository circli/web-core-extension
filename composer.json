{
  "name": "circli/web-core",
  "type": "circli-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Sundqvist",
      "email": "andreas@oak-valley.se"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "circli/contracts": "2.x-dev",
    "circli/core": "2.x-dev",
    "php-di/php-di": "^6.0",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "polus/adr": "5.x-dev",
    "polus/fast-router": "^2.0",
    "polus/relay-middleware-dispatcher": "^2.0",
    "laminas/laminas-diactoros": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~8.0",
    "phpstan/phpstan": "^0.12.49"
  },
  "autoload": {
    "psr-4": {
      "Circli\\WebCore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
